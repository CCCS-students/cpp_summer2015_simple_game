#include <iostream>
#include <string>


class Goblin
{
	
public:
	Goblin();
	~Goblin();
	Goblin (double, string, string, sting);
	void set_size(double sizeIn);
	int get_size();
	void set_color(string colorIn);
	int get_color();
	void set_weapon(string weaponIn);
	int get_weapon();
	void set_status(string statusIn);
	int get_status();

private:
	double size;
	string color;
	string weapon;
	string status;

};